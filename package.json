  "scripts": {
    "dev": "vinxi dev",
    "build": "bun run db:push && bun run db:seed && bun run generateContent && bunx dotenv -- vinxi build",
    "start": "vinxi start",
    "fix": "concurrently \"eslint . --fix\" \"prettier --write .\"",
    "generateContent": "bun run scripts/generateImageMaps.ts && bun run scripts/generateSlides.ts",
    "lint": "concurrently \"bun run tsc -b tsconfig.json\" \"eslint .\" \"prettier --check .\"",
    "test": "vitest --run",
    "test:watch": "vitest",
    "clean": "git clean -xdf -e .env -e local.db",
    "db:local:init": "bun run scripts/initDb.mjs",
    "db:studio": "drizzle-kit studio --port 3003 --verbose",
    "db:generate": "drizzle-kit generate && bun run generate",
    "db:push": "bun drizzle-kit push --force",
    "db:seed": "bun run scripts/seed.ts"
  },
